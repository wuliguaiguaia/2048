for(let i=0;i<rules.length;i++){
    if(rules[i].name == "warn" && rules[i].type == CSSRule.KEYFRAMES_RULE){
        
    }
}

5.addRule,deleteRule和findRule对keyframes对象进行操作
mysheet.insertRule("#blanc { color: white }", 0);
mysheet.deleteRule(index)

    // 添加规则 
    CSSrule(rule){
        let sheet = document.styleSheets[0];
        let rulesLen= sheet.cssRules.length;
        return{
            insert(){
                sheet.insertRule(rule,rulesLen-1);
            },
            remove(){
                sheet.delteRule(rulesLen-1);
            
            }
        }
    }

